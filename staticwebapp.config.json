{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/images/*",
      "/static/*",
      "/_next/*",
      "/api/*",
      "/*.{png,jpg,gif,webp,ico}"
    ]
  },
  "routes": [
    {
      "route": "/_next/static/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/static/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ],
  "platform": {
    "apiRuntime": "node:18"
  },
  "buildProperties": {
    "appLocation": "/domains/access",
    "outputLocation": ".next"
  }
}
